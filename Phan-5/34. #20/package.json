{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "nodejs",
  "main": "dist/App.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "dev": "nodemon",
    "start:debug": "nodemon --exec \"node --inspect -r ts-node/register ./src/App.ts\""
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules/**/*",
      "dist/**/*"
    ],
    "exec": "ts-node ./src/App.ts",
    "legacyWatch": true,
    "delay": 500,
    "verbose": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.7",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
